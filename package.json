{
  "name": "csharp-create-derived-class",
  "displayName": "C# Create Derived Class",
  "description": "Adds a Code Action to create a derived class for a C# class declaration",
  "version": "0.0.15",
  "publisher": "ilshatmukminov",
  "repository": {"url": "https://github.com/igoodwin/csharp-create-derived-class"},
  "license": "MIT",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createDerivedClass",
        "title": "Create derived class"
      },
      {
        "command": "extension.addPropertyToInterface",
        "title": "Add property to interface"
      },
      {
        "command": "extension.showClassMembers",
        "title": "Show class members"
      },
      {
        "command": "extension.navigateToNextClassMember",
        "title": "Go to next class member"
      },
      {
        "command": "extension.navigateToPreviousClassMember",
        "title": "Go to previous class member"
      }
    ],
    "languages": [
      {
        "id": "csharp",
        "extensions": [
          ".cs"
        ]
      }
    ],
    "keybindings": [
      {
        "command": "extension.showClassMembers",
        "key": "alt+shift+\\\\",
        "when": "editorTextFocus && editorLangId == csharp"
      },
      {
        "command": "extension.navigateToPreviousClassMember",
        "key": "ctrl+alt+up",
        "when": "editorTextFocus && editorLangId == csharp"
      },
      {
        "command": "extension.navigateToNextClassMember",
        "key": "ctrl+alt+down",
        "when": "editorTextFocus && editorLangId == csharp"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@types/vscode": "^1.70.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.0"
  },
  "dependencies": {
    "text-encoder": "^0.0.4"
  },
  "extensionDependencies": [
    "ms-dotnettools.csharp"
  ]
}
